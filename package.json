{
  "name": "vscode-switch-git-worktree",
  "displayName": "Switch Git Worktree",
  "description": "Easily switch between Git working trees",
  "publisher": "chrisdicarlo",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisdicarlo/vscode-switch-git-worktree.git"
  },
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:git-worktrees.switchWorktree",
    "onCommand:git-worktrees.switchWorktreeView",
    "onView:WorktreeList"
  ],
  "main": "./extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "WorktreeList",
          "name": "Worktrees",
          "contextualTitle": "Worktree list"
        }
      ]
    },
    "commands": [
      {
        "command": "git-worktrees.switchWorktree",
        "title": "Switch Worktree"
      },
      {
        "command": "git-worktrees.switchWorktreeView",
        "title": "Switch Worktree"
      }
    ]
    
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  }
}
